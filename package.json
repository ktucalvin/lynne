{
  "name": "lynne",
  "version": "0.6.0",
  "description": "A Discord bot built with Discord.js for simple moderation and music playback.",
  "main": "index.js",
  "private": true,
  "_moduleAliases": {
    "$config": "config.json",
    "$lib": "lib",
    "$structures": "lib/structures"
  },
  "dependencies": {
    "discord.js": "^11.6.3",
    "dotenv": "^8.2.0",
    "erlpack": "^0.1.3",
    "koa-compose": "^4.1.0",
    "libsodium-wrappers": "^0.7.6",
    "mocha": "^7.1.1",
    "mock-fs": "^4.11.0",
    "module-alias": "^2.2.2",
    "node-opus": "^0.3.3",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "uws": "^100.0.1",
    "ytdl-core": "^2.0.1",
    "ytdl-getinfo": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "mockery": "^2.1.0"
  },
  "scripts": {
    "test": "mocha",
    "test-all": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs mocha",
