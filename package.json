{
  "name": "mystia",
  "version": "0.5.6",
  "description": "A Discord bot built with Discord.js for simple moderation and music playback with an extensible framework.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "_moduleAliases": {
    "$config": "config.json",
    "$lib": "lib",
    "$structures": "lib/structures"
  },
  "dependencies": {
    "discord.js": "^11.4.2",
    "dotenv": "^5.0.1",
    "erlpack": "^0.1.0",
    "koa-compose": "^4.1.0",
    "libsodium-wrappers": "^0.7.4",
    "module-alias": "^2.1.0",
    "node-opus": "^0.2.9",
    "uws": "^9.148.0",
    "ytdl-core": "^0.28.3",
    "ytdl-getinfo": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "mocha": "^5.2.0",
    "mock-fs": "^4.7.0",
    "mockery": "^2.1.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.3.0"
  },
  "scripts": {
    "test": "mocha",
    "test-all": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs mocha",
    "test-coverage": "find . -not -path \"*node_modules*\" -name \"*test\" | xargs nyc --reporter=html --reporter=text mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ktucalvin@bitbucket.org/ktucalvin/mystia.git"
  },
  "keywords": [
    "discord"
  ],
  "author": "ktucalvin",
  "license": "MIT",
  "homepage": "https://bitbucket.org/ktucalvin/mystia#readme"
}
